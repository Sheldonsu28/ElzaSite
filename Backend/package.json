{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "nodemonConfig": {
    "exec": "node --experimental-specifier-resolution=node --use_strict ./src/server.js",
    "watch": [
      "src/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "scripts": {
    "start": "nodemon"
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "axios": "^0.27.2",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "mongoose": "^6.3.3",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typedi": "^0.10.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
